target 'Heroin' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for Heroin

#代码注入
pod 'XAspect'
#日志系统
pod 'CocoaLumberjack', '~>3.2.0'
#开发工具，可以用来Debug view，分析网络请求
pod 'FLEX', '~> 2.0', :configurations => ['Debug']
